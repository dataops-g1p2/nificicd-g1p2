# ============================================
# Docker Compose Files
# ============================================
# Ignore all compose files EXCEPT the ones we want to track
*.yml
*.yaml
# Allow specific compose files
!compose.development.yml
!compose.staging.yml
!compose.production.yml
!docker-compose*.yml
!docker-compose*.yaml
!compose.local.yml
# CRITICAL: Allow GitHub Actions workflow files!
!.github/**/*.yml
!.github/**/*.yaml

# ============================================
# Environment Variables & Secrets
# ============================================
# .env
.env.*


# Secret files
secrets/
*.pem
*.crt
*.cer
*.p12
*.pfx
*.secret

# Azure credentials
.azure/

# ============================================
# SSH Keys
# ============================================
# Ignore SSH keys everywhere by default
*.key
id_rsa
id_ed25519
*.ppk

# But ALLOW SSH keys in the ssh-keys directory
!ssh-keys/
!ssh-keys/*.key
!ssh-keys/*_key
!ssh-keys/*.pub
!ssh-keys/id_rsa
!ssh-keys/id_rsa.pub
!ssh-keys/id_ed25519
!ssh-keys/id_ed25519.pub

